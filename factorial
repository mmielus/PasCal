program fun;

procedure fun(n : integer);
var
result : integer;
i : integer;
begin
    if n < 0 then begin

    end else if n = 0 or n = 1 then begin

    end else begin
        result := 1;
        for i := 2 to n+1 do begin
            result := result * i;
        end;


        write(result);
    end;
end;

begin
   fun(5);
end.
